{
  "name": "hc-terch-frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "pages": "npx angular-cli-ghpages --dir=dist/hc_terch_frontend/browser",
    "build": "ng build --base-href /hc_tech-front/",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "20.3.6",
    "@angular/common": "20.3.2",
    "@angular/compiler": "20.3.2",
    "@angular/core": "20.3.2",
    "@angular/forms": "20.3.2",
    "@angular/platform-browser": "20.3.2",
    "@angular/router": "20.3.2",
    "@primeuix/themes": "1.2.5",
    "chart.js": "4.3.0",
    "ng2-charts": "5.0.0",
    "primeflex": "4.0.0",
    "primeicons": "7.0.0",
    "primeng": "20.2.0",
    "rxjs": "7.8.1",
    "tslib": "2.3.0"
  },
  "devDependencies": {
    "@angular/build": "20.3.3",
    "@angular/cli": "20.3.3",
    "@angular/compiler-cli": "20.3.2",
    "@types/jasmine": "5.1.0",
    "angular-cli-ghpages": "^2.0.3",
    "jasmine-core": "5.9.0",
    "karma": "6.4.0",
    "karma-chrome-launcher": "3.2.0",
    "karma-coverage": "2.2.0",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.1.0",
    "typescript": "5.9.2",
    "uuid": "^13.0.0"
  }
}
